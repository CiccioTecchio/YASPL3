double value, result;
def sqrt(in double value, out double result) {
	int i;
	result = value / 2;
	i = 0;
	while(i < 64) do {
		result = (result + (value / result)) / 2;
		i = i + 1;
	}
} 

def sqrt2(in double value, out double result) {
	int i;
	result = value / 2;
	i = 0;
	while(i < 64) do {
		result = (result + (value / result)) / 2;
		i = i + 1;
	}
} 
